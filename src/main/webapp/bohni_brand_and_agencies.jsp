<!-- Main section -->

<section id="top-section" style="background-image: webasset/img/what-we-do.jpg">
<div class="container">
<div class="row">
<div class="col-12">
<h1>The performance driven ad tech platform</h1>
<h3 class="mt-5">Request a Quote</h3>
</div>
<div class="col-12">
<form action="#">
<input type="text" name="name" placeholder="Name"> 
<input type="text" name="email" placeholder="Email"> 
<input type="text" name="company" placeholder="Company">
<button type="submit" name="submit">Submit</button> 
</form>
